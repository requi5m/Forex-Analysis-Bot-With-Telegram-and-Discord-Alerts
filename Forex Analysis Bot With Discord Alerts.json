    {
      "parameters": {
        "jsCode": "// n8n Code Node - Professional Liquidity & Range Analysis\nconst items = $input.all();\nlet results = [];\n\nfor (const item of items) {\n  const mainData = item.json.data;\n\n  // Güvenlik kontrolü\n  if (!mainData || !mainData.kline_list || !Array.isArray(mainData.kline_list)) {\n    results.push({ \n      json: { \n        error: \"Veri json.data.kline_list içinde bulunamadı!\",\n        debug: item.json \n      } \n    });\n    continue;\n  }\n\n  const list = mainData.kline_list;\n\n  // 1. Sayısal Dönüşümler\n  const volumes = list.map(k => parseFloat(k.volume || 0));\n  const highs = list.map(k => parseFloat(k.high_price));\n  const lows = list.map(k => parseFloat(k.low_price));\n\n  // 2. Range (Aralık) ve Hacim Hesaplamaları\n  const maxPrice = Math.max(...highs);\n  const minPrice = Math.min(...lows);\n  const totalVolume = volumes.reduce((a, b) => a + b, 0);\n  const avgVolume = totalVolume / list.length;\n\n  // 3. Likidite Verimlilik Skoru (Hacim / Fiyat Aralığı)\n  const priceRange = maxPrice - minPrice;\n  const liquidityScore = priceRange !== 0 ? (totalVolume / priceRange) : 0;\n\n  // 4. Likidite Durumu Belirleme\n  let status = \"thin\";\n  if (liquidityScore > 2500) status = \"moderate\";\n  if (liquidityScore > 10000) status = \"deep\";\n\n  // 5. Çıktı Formatı\n  results.push({\n    json: {\n      symbol: mainData.code || \"AUDUSD\",\n      high: maxPrice,\n      low: minPrice,\n      liquidity_score: parseFloat(liquidityScore.toFixed(2)),\n      liquidity_status: status,\n      avg_volume: parseFloat(avgVolume.toFixed(2)),\n      total_volume: totalVolume,\n      data_points: list.length\n    }\n  });\n}\n\nreturn results;"
      },
