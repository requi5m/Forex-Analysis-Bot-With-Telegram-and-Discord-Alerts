    {
      "parameters": {
        "jsCode": "// n8n Code Node\nconst items = $input.all();\nlet results = [];\n\nfor (const item of items) {\n  // Veri json.data içinden geliyorsa onu yakala\n  const mainData = item.json.data;\n\n  // Güvenlik kontrolü: data var mı ve içinde kline_list var mı?\n  if (!mainData || !mainData.kline_list || !Array.isArray(mainData.kline_list)) {\n    results.push({ \n      json: { \n        error: \"Veri json.data.kline_list içinde bulunamadı!\",\n        debug: item.json // Neyin geldiğini görmek için\n      } \n    });\n    continue;\n  }\n\n  const list = mainData.kline_list;\n\n  // 1. Değerleri sayıya çevirerek listele\n  const highs = list.map(k => parseFloat(k.high_price));\n  const lows = list.map(k => parseFloat(k.low_price));\n  const closes = list.map(k => parseFloat(k.close_price));\n\n  // 2. En büyük (Resistance) ve En küçük (Support) bul\n  const resistance = Math.max(...highs);\n  const support = Math.min(...lows);\n\n  // 3. Trend Ölçümü\n  const firstPrice = closes[0];\n  const lastPrice = closes[closes.length - 1];\n  \n  let trend = \"SIDEWAYS\";\n  if (lastPrice > firstPrice) trend = \"bullish\";\n  if (lastPrice < firstPrice) trend = \"bearish\";\n\n  // 4. Çıktı formatı\n  results.push({\n    json: {\n      symbol: mainData.code || \"AUDUSD\",\n      support: support,\n      resistance: resistance,\n      trend: trend,\n      last_price: lastPrice,\n      data_points: list.length\n    }\n  });\n}\n\nreturn results;"
      },
